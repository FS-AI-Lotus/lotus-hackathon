{
  "dashboard": {
    "title": "Coordinator Service - Monitoring Dashboard",
    "uid": "coordinator-monitoring",
    "tags": ["coordinator", "team4", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Requests per Second",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            },
            "expr": "sum(rate(http_requests_total{service=\"coordinator\"}[1m])) by (route, method)",
            "legendFormat": "{{route}} - {{method}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "legend": {"show": true, "values": false, "current": true}
      },
      {
        "id": 2,
        "title": "Total Requests",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(http_requests_total{service=\"coordinator\"}[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "(sum(rate(http_requests_total{service=\"coordinator\",status=~\"5..\"}[5m])) / sum(rate(http_requests_total{service=\"coordinator\"}[5m]))) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Error Rate %", "max": 100, "min": 0},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "legend": {"show": true}
      },
      {
        "id": 4,
        "title": "p95 Latency by Route",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{service=\"coordinator\"}[5m])) by (le, route, method))",
            "legendFormat": "{{route}} - {{method}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency (seconds)"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "legend": {"show": true, "values": false, "current": true}
      },
      {
        "id": 5,
        "title": "p50 Latency",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{service=\"coordinator\"}[5m])) by (le))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3
          }
        }
      },
      {
        "id": 6,
        "title": "p95 Latency",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{service=\"coordinator\"}[5m])) by (le))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 2, "color": "yellow"},
              {"value": 5, "color": "red"}
            ]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3
          }
        }
      },
      {
        "id": 7,
        "title": "Uptime",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "time() - process_start_time_seconds{job=\"coordinator\"}",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 0
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Value"},
              "properties": [
                {
                  "id": "unit",
                  "value": "dtdurations"
                }
              ]
            }
          ]
        }
      },
      {
        "id": 8,
        "title": "Service Registrations Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 9, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(coordinator_service_registrations_total[5m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Registrations/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "legend": {"show": true, "values": false, "current": true},
        "stack": true,
        "nullPointMode": "null"
      },
      {
        "id": 9,
        "title": "Total Service Registrations",
        "type": "stat",
        "gridPos": {"h": 8, "w": 3, "x": 15, "y": 16},
        "targets": [
          {
            "expr": "sum(increase(coordinator_service_registrations_total[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 10,
        "title": "Routing Operations - Success vs Failed",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(coordinator_routing_operations_total[5m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Operations/sec"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "legend": {"show": true, "values": false, "current": true},
        "stack": true,
        "nullPointMode": "null"
      },
      {
        "id": 11,
        "title": "Total Routing Operations",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "sum(increase(coordinator_routing_operations_total[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 12,
        "title": "HTTP Status Code Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{service=\"coordinator\"}[5m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          },
          "pieType": "pie"
        }
      },
      {
        "id": 13,
        "title": "Active Alerts",
        "type": "alertlist",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [],
        "options": {
          "show": "current",
          "sortOrder": 1,
          "stateFilter": ["alerting", "pending"]
        }
      },
      {
        "id": 14,
        "title": "Request Rate by Method",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{service=\"coordinator\"}[5m])) by (method)",
            "legendFormat": "{{method}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "decimals": 2
          }
        }
      },
      {
        "id": 15,
        "title": "Error Count by Status",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(http_errors_total{service=\"coordinator\"}[5m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 2
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "links": []
  }
}

